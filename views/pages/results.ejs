


<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('../partials/head.ejs') %>
</head>

<body>
    <header>
        <%- include('../partials/header.ejs') %>

    </header>

    <h1>Your matches:</h1>

 
    <ul>
        <% match.forEach(function(match) { %>
          <li>
            <strong>name:<%= match.name %></strong>
            location: <%= match.location %>,
            interest: <%= match.interest %>,
            duration of walk:  <%= match.time %>
          </li>
        <% }); %>
      </ul>
     

    <footer>
    
    <!--working with include: including a footer from the partial directory-->
        <%- include('../partials/footer.ejs') %>
    </footer>
</body>

</html>